{
    "name": "developertugrul/paytr-laravel-client",
    "description": "Laravel için güncel, güvenli ve kapsamlı PayTR ödeme entegrasyon paketi. Güncel PayTR API dokümantasyonuna tam uyumlu.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "paytr",
        "payment",
        "turkish",
        "payment-gateway",
        "credit-card",
        "iframe",
        "direct-api"
    ],
    "authors": [
        {
            "name": "Tugrul Developer",
            "email": "info@developertugrul.com",
            "homepage": "https://developertugrul.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": ">=8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Paytr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paytr\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paytr\\PaytrServiceProvider"
            ],
            "aliases": {
                "Paytr": "Paytr\\Facades\\Paytr"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}